{
  "name": "eckart-web",
  "version": "1.0.0",
  "description": "easyKart webapp implementation",
  "author": "sandeep pulikallu",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spulikallu/ecKart-web.git"
  },
  "scripts": {
    "prestart": "rimraf build && rimraf ./stats.json",
    "start": "webpack-dev-server --open --mode development",
    "prebuild": "rimraf build && rimraf ./stats.json",
    "build": "webpack --mode production --progress --display-error-details",
    "check-eslint-config": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "check-stylelint-config": "stylelint-config-prettier-check",
    "lint-code": "eslint 'src/js/**/*.js'",
    "lint-style": "stylelint 'src/css/**/*.{css,scss}'",
    "analyze": "webpack-bundle-analyzer ./stats.json"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "csso-webpack-plugin": "^1.0.0-beta.12",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.4.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-sass-guidelines": "^6.1.0",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.11.1",
    "webpack": "^4.41.0",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2",
    "webpack-stats-plugin": "0.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn check-eslint-config && yarn lint-code && yarn check-stylelint-config && yarn lint-style"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "keywords": [
    "es6",
    "webpack",
    "add-to-cart",
    "e-commerce",
    "ecKart"
  ],
  "bugs": {
    "url": "https://github.com/spulikallu/ecKart-web/issues"
  },
  "homepage": "https://github.com/spulikallu/ecKart-web#readme",
  "dependencies": {
    "nouislider": "^14.1.1"
  }
}
